{
  "name": "sweetalert2-react-content",
  "version": "0.2.1",
  "repository": "sweetalert2/sweetalert2-react-content",
  "description": "Dynamic content in SweetAlert2, the React way",
  "main": "dist/main.bundle.js",
  "jsnext:main": "dist/main.bundle.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:tests": "karma start karma.conf.js",
    "dev:sandbox": "webpack-serve webpack.config.sandbox.js --no-hot --no-reload --open",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "test-only": "karma start karma.conf.js --single-run --report-coverage",
    "test": "npm run lint && npm run test-only",
    "build": "del-cli dist && webpack --config webpack.config.dist.js",
    "release": "npm run build && semantic-release"
  },
  "keywords": [
    "sweetalert",
    "sweetalert2",
    "react",
    "content"
  ],
  "author": {
    "name": "Matthew Francis Brunetti",
    "email": "zenflow87@gmail.com"
  },
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sweetalert2": "^7.14.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^4.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.18.1",
    "eslint-config-zenflow": "^1.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^3.0.4",
    "jasmine": "^3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.13",
    "prettier": "^1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-release": "^15.0.0",
    "sweetalert2": "^7.14.0",
    "webpack": "^4.0.1",
    "webpack-serve": "^0.2.0"
  },
  "eslintConfig": {
    "extends": [
      "zenflow"
    ]
  },
  "eslintIgnore": [
    "coverage/**",
    "dist/**"
  ]
}
