{
  "name": "sweetalert2-react-content",
  "version": "0.4.0",
  "description": "Dynamic content in SweetAlert2, the React way",
  "main": "dist/sweetalert2-react-content.cjs.js",
  "module": "dist/sweetalert2-react-content.es.js",
  "browser": "dist/sweetalert2-react-content.umd.js",
  "types": "src/sweetalert2-react-content.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*.{js,d.ts}",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev:tests": "karma start karma.conf.js",
    "dev:sandbox": "webpack-serve webpack.config.sandbox.js --no-hot --no-reload --open",
    "check-dts": "tsc src/sweetalert2-react-content.d.ts --lib dom,es6",
    "lint": "run-s lint:*",
    "lint:es": "zenflow-lint-js",
    "lint:ts": "tslint -t codeFrame 'src/**/*.d.ts'",
    "fix": "run-s fix:*",
    "fix:es": "zenflow-lint-js --fix",
    "fix:ts": "npm run lint:ts --fix",
    "test-only": "karma start karma.conf.js --single-run --report-coverage",
    "test": "npm run check-dts && npm run lint && npm run test-only",
    "build": "zenflow-build-js-lib --minify --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sweetalert2/sweetalert2-react-content.git"
  },
  "keywords": [
    "sweetalert",
    "sweetalert2",
    "react",
    "content"
  ],
  "author": {
    "name": "Matthew Francis Brunetti",
    "email": "zenflow87@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sweetalert2/sweetalert2-react-content/issues"
  },
  "homepage": "https://github.com/sweetalert2/sweetalert2-react-content#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sweetalert2": "^7.14.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.0",
    "@semantic-release/git": "^4.0.0",
    "@types/react": "^16.0.40",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "html-webpack-plugin": "^3.0.6",
    "jasmine": "^3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.13",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-release": "^15.0.0",
    "sweetalert2": "^7.15.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-serve": "^0.1.5",
    "zenflow-build-js-lib": "^1.0.0",
    "zenflow-lint-js": "^2.0.0"
  },
  "zenflowConfig": {
    "build": {
      "globals": {
        "sweetalert2": "swal",
        "react": "React",
        "react-dom": "ReactDOM"
      }
    }
  },
  "greenkeeper": {
    "ignore": [
      "webpack-serve"
    ]
  }
}
